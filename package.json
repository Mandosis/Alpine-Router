{
  "name": "alpine-router",
  "version": "0.1.0",
  "description": "A client-side router with no dependencies featuring child routes and built with the ES2015 JavaScript feature set using TypeScript.",
  "main": "src/router.ts",
  "jsnext:main": "dist/es6/router.js",
  "scripts": {
    "test": "rimraf .ava/**/* && tsc -p tsconfig.test.json && ava",
    "clean": "rimraf .ava/**/*",
    "ava": "ava",
    "ava-watch": "cls && ava -w",
    "test-watch": "tsc -p tsconfig.test.json -w",
    "build": "webpack -p && tsc",
    "watch": "webpack -w",
    "prepublish": "npm run build"
  },
  "ava": {
    "files": [
      ".ava/test/**/*.spec.js"
    ],
    "source": [
      ".ava/src/**/*.js"
    ],
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mandosis/Alpine-Router.git"
  },
  "author": "Chris Rabuse <me@chrisrabuse.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mandosis/Alpine-Router/issues"
  },
  "homepage": "https://github.com/Mandosis/Alpine-Router#readme",
  "devDependencies": {
    "@types/whatwg-fetch": "^0.0.33",
    "ava": "^0.17.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.10",
    "browser-env": "^2.0.19",
    "rimraf": "^2.5.4",
    "ts-loader": "^2.0.1",
    "typescript": "^2.1.5",
    "webpack": "^2.2.1"
  }
}
